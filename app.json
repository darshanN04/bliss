{
  "expo": {
    "name": "bliss",
    "slug": "bliss",
    "owner": "bliss-mental-wellness-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/logo.png",
    "scheme": "bliss",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/logo.png",
      "foregroundImage": "./assets/icons/logo.png",
      "resizeMode": "contain",
      "imageWidth": 150,
      "imageheight": 150,
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/logo.png",
        "resizeMode": "contain",
        "imageWidth": 150,
        "imageHeight": 150,
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9cca2757-3241-4344-864a-318355266682"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9cca2757-3241-4344-864a-318355266682"
    }
  }
}
